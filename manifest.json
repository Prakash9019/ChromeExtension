{
    "name": "Color picker",
    "description": "A color picker extension",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "options_page": "options.html",
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "./picker_16.png"
    }
}